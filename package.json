{
  "name": "ai-rag-test",
  "version": "0.0.1",
  "description": "AI RAG Test Application",
  "main": "index.js",
  "author": "Gabriel Bernal<gabrielbernalp@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=22 <23",
    "yarn": ">=1.22.10 <2"
  },
  "scripts": {
    "dev": "yarn build && node scripts/watch.js",
    "build": "node scripts/build.js",
    "start": "node build/server.js",
    "compile": "tsc --noEmit",
    "lint": "eslint -c .eslintrc.json --ext .ts,.tsx ./src",
    "prepare": "husky install",
    "precommit": "concurrently --names \"lint,compile\" \"yarn lint\" \"yarn compile\"",
    "prepush": "lint-staged",
    "test": " jest --config jest.config.js --runInBand",
    "release": "./scripts/release.sh"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@google/genai": "^1.3.0",
    "@langchain/core": "^0.3.23",
    "@langchain/google-genai": "^0.2.9",
    "@langchain/langgraph": "^0.2.33",
    "@langchain/mongodb": "^0.1.0",
    "@langchain/openai": "^0.3.14",
    "@langchain/textsplitters": "^0.1.0",
    "@types/pdf-parse": "^1.1.5",
    "axios": "^1.9.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "lru-cache": "^10.0.0",
    "luxon": "^3.3.0",
    "mongoose": "^7.2.4",
    "pdf-parse": "^1.1.1",
    "uuid": "^11.0.3",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.3.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.1",
    "esbuild": "^0.25.4",
    "esbuild-node-externals": "^1.7.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.0.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
